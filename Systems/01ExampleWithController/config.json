{
	"Sources": {
		"xGraph": "{xGraph}"
	},
	"Modules": {
		"Server": {
			"Module": "xGraph.Web.Server",
			"Source": "xGraph",
			"Par": {
				"ApexList": {
					"Controller":"$Controller"
				},				
				"Port": 8080,
				"Url": "Paint",
				"HTML": "@file: Paint.html",
				"Paint": "@system: browser.json"
			}
		},
		"Controller": {
			"Module": "xGraph.Controller",
			"Source": "xGraph",
			"Par":{
				"Server":"$3DViewProxy"
			}
		},
		"3DViewProxy": {
			"Module":"xGraph.Web.ServerProxy",
			"Source":"xGraph",
			"Par":{
				"Link": "3DView",
				"Server":"$Server"			
			}	
		}
	}
}
{
	"Sources": {
		"local": "{local}"
	},
	"Modules": {
		"DataStore": {
			"Module": "xGraphDev.Extensions.Modules.SimpleDataStore",
			"Source": "local",
			"Par": {}
		},
		"Server": {
			"Module": "xGraphDev.Core.Modules.xGraph.Proxy",
			"Source": "local",
			"Par": {
				"Role": "Server",
				"Link": "$DataStore",
				"Encrypt": false,
				"AutoSave": false
			}
		}
	}
}
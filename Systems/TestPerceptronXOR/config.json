{
	"Sources": {
		"xGraph": "{xGraph}"
	},
	"Modules": {
		"NeuralNet": {
			"Module": "xGraph.Perceptron",
			"Source": "xGraph",
			"Par": {
				"NetworkDimensions": [
					2,
					3,
					1
				],
				"InitializeOnSetup": true,
				"TrainingSetSize": 20
			}
		},
		"Xor": {
			"Module": "xGraph.XOR",
			"Source": "xGraph",
			"Par": {
				"Brain": "$NeuralNet"
			}
		}
	}
}
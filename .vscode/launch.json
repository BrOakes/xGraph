	"version": "0.2.0",
	"configurations": [
		{
			"name": "Win | RunModuleServer",
			"type": "node",
			"request": "launch",
			"cwd": "${workspaceRoot}/Work/ModuleServer",
			"program": "C:/xGraph/Nexus/Nexus/Nexus.js",
			"args": [
				"Config=config.json",
				"xGraph=C:/xGraph"
			],
			"env": {
				"NODE_PATH": "node_modules"
			}
		},
		{
			"name": "Win | Plexus",
			"type": "node",
			"request": "launch",
			"cwd": "${workspaceRoot}/Work/Plexus",
			"program": "C:/xGraph/Nexus/Nexus/Nexus.js",
			"args": [
				"Config=config.json",
				"xGraph=C:/xGraph"
			],
			"env": {
				"NODE_PATH": "node_modules"
			}
		}
	]
}
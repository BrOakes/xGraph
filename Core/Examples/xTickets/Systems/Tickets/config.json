{
	"Sources": {
		"local": "{local}",
		"Core": "{Core}"
	},
	"Modules": {
		"Google": {
			"Module": "Google",
			"Source": "local",
			"Par": {
				"ClientID": "",
				"ClientSecret": ""
			}
		},
		"GoogleLoginProvider": {
			"Module": "GoogleLoginProvider",
			"Source": "local",
			"Par": {}
		},
		"TicketGenSurrogate": {
			"Module": "GenModuleSurrogate",
			"Source": "local",
			"Par": {
				"ModuleDefinition": {
					"Top": {
						"Module": "Router",
						"Source": "local",
						"Par": {
							"Extends": "$Ticket"
						}
					},
					"Ticket": {
						"Module": "Ticket",
						"Par": {
							"List": "$Tickets",
							"AuthServer": "$AuthServer",
							"Broadcast": "$Top"
						}
					}
				},
				"Server": "$Server"
			}
		},
		"AuthServer": {
			"Module": "AuthenticationServer",
			"Source": "local",
			"Par": {
				"Google": "$Google",
				"LinkedModules": []
			}
		},
		"TicketsServerProxy": {
			"Module": "xGraph.ServerProxy",
			"Source": "Core",
			"Par": {
				"Link": "Tickets",
				"Server": "$Server"
			}
		},
		"Tickets": {
			"Module": "Router",
			"Source": "local",
			"Par": {
				"Extends": "$TicketsSet",
				"Table": [
					"$TicketsServerProxy"
				]
			}
		},
		"TicketsSet": {
			"Module": "Set",
			"Source": "local",
			"Par": {
				"List": [],
				"Broadcast": "$Tickets"
			}
		},
		"GoogleProxy": {
			"Module": "xGraph.Proxy",
			"Source": "Core",
			"Par": {
				"Role": "Server",
				"Link": "$Google",
				"Port": 25000,
				"Pool": "$ProxyPool",
				"Encrypt": false
			}
		},
		"TicketsProxy": {
			"Module": "xGraph.Proxy",
			"Source": "Core",
			"Par": {
				"Role": "Server",
				"Link": "$Tickets",
				"Port": 25001,
				"Pool": "$ProxyPool",
				"Encrypt": false
			}
		},
		"AuthServerProxy": {
			"Module": "xGraph.Proxy",
			"Source": "Core",
			"Par": {
				"Role": "Server",
				"Link": "$AuthServer",
				"Port": 25002,
				"Pool": "$ProxyPool",
				"Encrypt": false
			}
		},
		"Server": {
			"Module": "xGraph.Proxy",
			"Source": "Core",
			"Par": {
				"Role": "Client",
				"Host": "127.0.0.1",
				"Port": 25005,
				"Poll": true,
				"Encrypt": false
			}
		},
		"TicketGenSurrogateProxy": {
			"Module": "xGraph.Proxy",
			"Source": "Core",
			"Par": {
				"Role": "Server",
				"Link": "$TicketGenSurrogate",
				"Port": 25003,
				"Pool": "$ProxyPool",
				"Encrypt": false
			}
		},
		"GoogleLoginProviderProxy": {
			"Module": "xGraph.Proxy",
			"Source": "Core",
			"Par": {
				"Role": "Server",
				"Link": "$GoogleLoginProvider",
				"Port": 25004,
				"Encrypt": false
			}
		},
		"ProxyPool": {
			"Module": "ProxyPool",
			"Source": "local",
			"Par": {
				"Port": 27000
			}
		},
		"Deferred": [
			{
				"Module": "Ticket",
				"Source": "local"
			}
		]
	}
}

{
	"Sources": {
		"local": "{local}",
		"Core": "{Core}"
	},
	"Modules": {
		"Header": {
			"Module": "Views.HTMLView",
			"Source": "local",
			"Par": {
				"HTML": "@file: loggedout.html"
			}
		},
		"Root": {
			"Module": "Modules.xGraph.RootView",
			"Source": "Core",
			"Par": {
				"Layout": {
					"View": "$Header",
					"Children": [
						"$AuthView",
						"$LoginView"
					]
				}
			}
		},
		"LoginView": {
			"Module": "Modules.LoginView",
			"Source": "local",
			"Par": {
				"Server": "$AuthServer",
				"Google": "$Google",
				"Redirect": "login",
				"Text": "Logging Out . . .",
				"Logout": true
			}
		},
		"Google": {
			"Module": "Modules.xGraph.WebProxy",
			"Source": "Core",
			"Par": {
				"Link": "Google"
			}
		},
		"AuthServer": {
			"Module": "Modules.xGraph.WebProxy",
			"Source": "Core",
			"Par": {
				"Link": "AuthServer"
			}
		}
	}
}
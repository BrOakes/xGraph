{
	"Sources": {
		"local": "{local}",
		"Core": "{Core}"
	},
	"Modules": {
		"Header": {
			"Module": "Views.HTMLView",
			"Source": "local",
			"Par": {
				"HTML": "@file: layout.html"
			}
		},
		"Root": {
			"Module": "Modules.xGraph.RootView",
			"Source": "Core",
			"Par": {
				"Layout": {
					"View": "$Header",
					"Children": [
						"$AuthView",
						"$Button",
						"$Table"
					]
				}
			}
		},
		"PreFilter": {
			"Module": "Modules.DataFilter",
			"Source": "local",
			"Par": {
				"Source": "$Tickets",
				"DataKey": "Status",
				"DoesNotEqual": "Closed"
			}
		},
		"TicketsFilter": {
			"Module": "Modules.CookiesFilter",
			"Source": "local",
			"Par": {
				"Source": "$PreFilter",
				"DataKey": "AuthorEmail",
				"CookiesKey": "xGraph-Email"
			}
		},
		"Button": {
			"Module": "Views.Paper.Button",
			"Source": "local",
			"Par": {
				"GenModuleSurrogate": "$TicketGenSurrogate",
				"Text": "Create Ticket"
			}
		},
		"GoogleLoginRefresher": {
			"Module": "Modules.GoogleLoginRefresher",
			"Source": "local",
			"Par": {
				"Google": "$Google"
			}
		},
		"Google": {
			"Module": "Modules.xGraph.WebProxy",
			"Source": "Core",
			"Par": {
				"Link": "Google"
			}
		},
		"CommandAuthenticator": {
			"Module": "Modules.CommandAuthenticator",
			"Source": "local",
			"Par": {}
		},
		"AuthView": {
			"Module": "Modules.AuthenticationView",
			"Source": "local",
			"Par": {}
		},
		"Table": {
			"Module": "Modules.xGraph.TableView",
			"Source": "Core",
			"Par": {
				"Source": "$TicketsFilter",
				"Evoke": "View",
				"Columns": [
					{
						"Name": "Summary",
						"Key": "Summary"
					},
					{
						"Name": "Responsibility",
						"Key": "AssignedToDisplayName"
					},
					{
						"Name": "Due Date",
						"Format": "Date",
						"Key": "DueBy"
					},
					{
						"Name": "Priority",
						"Key": "Priority",
						"Enum": {
							"1": "Lowest",
							"2": "Low",
							"3": "Normal",
							"4": "High",
							"5": "Highest"
						}
					},
					{
						"Name": "Severity",
						"Key": "Severity",
						"Enum": {
							"1": "Feature Request",
							"2": "Mild",
							"3": "Moderate",
							"4": "Severe",
							"5": "Critical"
						}
					},
					{
						"Name": "Status",
						"Key": "Status"
					}
				]
			}
		},
		"Server": {
			"Module": "Modules.xGraph.WebProxy",
			"Source": "Core",
			"Par": {
				"Link": "Server"
			}
		},
		"Tickets": {
			"Module": "Modules.xGraph.WebProxy",
			"Source": "Core",
			"Par": {
				"Link": "Tickets"
			}
		},
		"TicketGenSurrogate": {
			"Module": "Modules.xGraph.WebProxy",
			"Source": "Core",
			"Par": {
				"Link": "TicketGenSurrogate"
			}
		},
		"Deferred": [
			{
				"Module": "Views.Paper.BottomSheet",
				"Source": "local"
			},
			{
				"Module": "Views.TicketEditView",
				"Source": "local"
			},
			{
				"Module": "Views.TicketHistoryView",
				"Source": "local"
			},
			{
				"Module": "Views.TicketCommentsView",
				"Source": "local"
			},
			{
				"Module": "Views.TicketView",
				"Source": "local"
			}
		]
	}
}

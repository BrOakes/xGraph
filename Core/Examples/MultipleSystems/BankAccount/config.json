{
    "Sources": {
        "xGraph": "{xGraph}"
    },
    "Modules": {
        "Bank": {
            "Module": "Modules.xGraph.BankAccount",
            "Source": "xGraph"
        },
        "BankServer": {
            "Module": "Modules.xGraph.Proxy",
            "Source": "xGraph",
            "Par": {
                "Port": 27002,
                "Role": "Server",

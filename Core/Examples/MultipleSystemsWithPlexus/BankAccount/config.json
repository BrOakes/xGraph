{
    "Sources": {
        "xGraph": "{xGraph}"
    },
    "Modules": {
        "Bank": {
            "Module": "xGraph.Modules.BankAccount",
            "Source": "xGraph",
            "Par": {
                "Balance": "10"
            }
        }, 
        "BankServer": {
            "Module": "xGraph.Modules.Proxy",
            "Source": "xGraph",
            "Par": {
                "Plexus": "$Plexus", 
                "Name": "Bank", 
                "Chan": "Bank", 
                "Link": "$Bank", 
                "Role": "Server"
            }
        }, 
        "Plexus": {
            "Module": "xGraph.Modules.Proxy",
            "Source": "xGraph",
            "Par": {
                "Plexus": "$Plexus", 
                "Name": "Bank", 
                "Chan": "Plexus", 
                "Link": "$Bank", 
                "Role": "Client"
            }
        }
    }
}
{
    "Sources": {
        "xGraph": "{xGraph}"
    },
    "Modules": {
        "BankPatron": {
            "Module": "Modules.BankPatron",
            "Source": "xGraph",
            "Par": {
                "BankAccount": "$BankServer"
            }
        },
        "BankServer": {
            "Module": "Modules.Proxy",
            "Source": "xGraph",
            "Par": {
                "Chan": "Bank",
                "Link": "$BankPatron",
                "Role": "Client"
            }
        }
    }
}
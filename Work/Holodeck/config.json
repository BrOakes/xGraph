{
  "xGraph": "{xGraph}",
  "Cache": "cache",
  "Redirect":{
	"Nxs.js":"{xGraph}/Nexus/Nexus/Nxs.js",
	"jQuery.js":"../Root/Scripts/jquery-2.1.1.min.js",
	"three.js":"../Root/Scripts/three.min-r82.js",
	"jszip.js":"../Root/Scripts/jszip.min_3.1.3.js",
	"async.js":"../Root/Scripts/async.js",
	"pngjs2.js":"../Root/Scripts/pngjs2.js",
	"PNGReader.js":"../Root/Scripts/PNGReader.js"
  },
  "Modules": {
	"Nexus": {
	  "Module": "xGraph:Nexus/Nexus",
	  "Par": {
	  }
	},
	"Server": {
	  "Module": "xGraph:Holodeck/Server",
	  "Par": {
		"Apex":{
		  "Scene":"$Scene"
		},
		"Nxs":"{xGraph}/Nexus/Nexus/Nxs2.js",
		"Port": 8080,
		"Url": "Paint"
	  }
	},
	"Scene":{
	  "Module":"xGraph:Scene/Scene",
	  "Par":{
		"Inst":[
		  "$Terrain"
		]
	  }
	},
	"Terrain": {
	  "Module":"xGraph:Scene/Instance",
	  "Par":{
		"Model":"$Flatland",
		"Name":"Flatland",
		"Role":"Terrain",
		"Inst":[
		  "$Model1"
		]
	  }
	},
	"Flatland": {
	  "Module":"xGraph:Scene/Flatland",
	  "Par":{
		"Size":[100, 100],
		"TextureSize":[25,25],
		"Texture":"sand-12.png"
	  }
	},
	"Model1": {
	  "Module":"xGraph:Scene/Instance",
	  "Par":{
		"Model":"$Model",
		"Name":"Geo.101Plants.Cactus3",
		"Role":"Artifact",
		"Inst":[
		]
	  }
	},
	"Model": {
	  "Module":"xGraph:Scene/ModelServer",
	  "Par":{
		"Archive":"../Root/Models"
	  }
	}
  }
}